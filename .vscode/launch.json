  "version": "0.2.0",
  "configurations": [
    {
      "name": "boss dev",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define-from-file", "env/.env","--dart-define-from-file","env/uat.env"]
    },
    {
      "name": "boss (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--dart-define", "env=fat"]
    },
    {
      "name": "boss (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--dart-define", "env=uat"]
    },
    {
        "name": "boss (release mode)",
        "request": "launch",
        "type": "dart",
        "flutterMode": "release",
        "args": ["--dart-define", "env=prod"]
      }
  ]
}
